{
  "start_state": "S0",
  "final_states": {
    "S_IDENTIFIER": "IDENTIFIER",
    "S_NUMBER": "NUMBER",
    "S_ASSIGN": "ASSIGN_OPERATOR",
    "S_COLON": "COLON",
    "S_LT": "RELATIONAL_OPERATOR",
    "S_LE": "RELATIONAL_OPERATOR",
    "S_NE": "RELATIONAL_OPERATOR",
    "S_GT": "RELATIONAL_OPERATOR",
    "S_GE": "RELATIONAL_OPERATOR",
    "S_EQ": "RELATIONAL_OPERATOR",
    "S_SEMICOLON": "SEMICOLON",
    "S_COMMA": "COMMA",
    "S_DOT": "DOT",
    "S_RANGE": "RANGE_OPERATOR",
    "S_LPAREN": "LPARENTHESIS",
    "S_RPAREN": "RPARENTHESIS",
    "S_LBRACKET": "LBRACKET",
    "S_RBRACKET": "RBRACKET",
    "S_PLUS": "ARITHMETIC_OPERATOR",
    "S_MINUS": "ARITHMETIC_OPERATOR",
    "S_MULTIPLY": "ARITHMETIC_OPERATOR",
    "S_DIVIDE": "ARITHMETIC_OPERATOR",
    "S_STRING_LITERAL": "STRING_LITERAL"
  },
  "keywords": [
    "program", "var", "begin", "end", "if", "then", "else",
    "while", "do", "for", "to", "downto", "integer", "real",
    "boolean", "char", "array", "of", "procedure",
    "function", "const", "type"
  ],
  "logical_operators": ["and", "or", "not"],
  "arithmetic_keywords": ["div", "mod"],
  "transitions": {
    "S0": {
      "letter": "S_IDENTIFIER",
      "digit": "S_NUMBER",
      ":": "S_COLON_WAIT",
      "<": "S_LT_WAIT",
      ">": "S_GT_WAIT",
      "=": "S_EQ",
      "(": "S_LPAREN_OR_COMMENT",
      ")": "S_RPAREN",
      "{": "S_COMMENT_BRACE",
      "'": "S_STRING_START",
      ";": "S_SEMICOLON",
      ",": "S_COMMA",
      ".": "S_DOT_WAIT",
      "[": "S_LBRACKET",
      "]": "S_RBRACKET",
      "+": "S_PLUS",
      "-": "S_MINUS",
      "*": "S_MULTIPLY",
      "/": "S_DIVIDE"
    },
    "S_IDENTIFIER": {
      "letter": "S_IDENTIFIER",
      "digit": "S_IDENTIFIER"
    },
    "S_NUMBER": {
      "digit": "S_NUMBER"
    },
    "S_COLON_WAIT": {
      "=": "S_ASSIGN"
    },
    "S_LT_WAIT": {
      "=": "S_LE",
      ">": "S_NE"
    },
    "S_GT_WAIT": {
      "=": "S_GE"
    },
    "S_LPAREN_OR_COMMENT": {
        "*": "S_COMMENT_STAR"
    },
    "S_COMMENT_STAR": {
        "*": "S_COMMENT_STAR_END_WAIT",
        "any_not_*": "S_COMMENT_STAR"
    },
    "S_COMMENT_STAR_END_WAIT": {
        ")": "S0",
        "any_not_)": "S_COMMENT_STAR"
    },
    "S_COMMENT_BRACE": {
        "}": "S0",
        "any_not_}": "S_COMMENT_BRACE"
    },
    "S_DOT_WAIT": {
        ".": "S_RANGE"
    },
    "S_STRING_START": {
      "any_not_'": "S_IN_STRING"
    },
    "S_IN_STRING": {
      "'": "S_STRING_LITERAL",
      "any_not_'": "S_IN_STRING"
    }
  }
}